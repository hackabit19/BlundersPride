{
    "version": 2,
    "builds": [{ "src": "index.js", "use": "@now/node-server" }],
    "routes": [
        {
            "src": "/api/users/register",
            "dest": "/index.js",
            "methods": ["POST"]
        },
        {
            "src": "/api/users/login",
            "dest": "/index.js",
            "methods": ["POST"]
        },
        {
            "src": "/api/missingPersons/reportMissing",
            "dest": "/index.js",
            "methods": ["POST"]
        },
        {
            "src": "/api/missingPersons/all",
            "dest": "/index.js",
            "methods": ["GET"]
        },
        {
            "src": "/api/missingPersons/:id",
            "dest": "/index.js",
            "methods": ["GET"]
        }
    ]
}
